{
  "multipart": [
    {
      "apply": {
        "model": "windswept:block/stripped_chestnut_post"
      },
      "when": {
        "axis": "y"
      }
    },
    {
      "apply": {
        "model": "windswept:block/stripped_chestnut_post",
        "x": 90,
        "y": 90
      },
      "when": {
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "windswept:block/stripped_chestnut_post",
        "x": 90
      },
      "when": {
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "blueprint:block/chain_small"
      },
      "when": {
        "chain_down": "true"
      }
    },
    {
      "apply": {
        "model": "blueprint:block/chain_small_top"
      },
      "when": {
        "chain_up": "true"
      }
    },
    {
      "apply": {
        "model": "blueprint:block/chain_small_top",
        "x": 90
      },
      "when": {
        "chain_north": "true"
      }
    },
    {
      "apply": {
        "model": "blueprint:block/chain_small",
        "x": 90
      },
      "when": {
        "chain_south": "true"
      }
    },
    {
      "apply": {
        "model": "blueprint:block/chain_small_top",
        "x": 90,
        "y": 90
      },
      "when": {
        "chain_east": "true"
      }
    },
    {
      "apply": {
        "model": "blueprint:block/chain_small",
        "x": 90,
        "y": 90
      },
      "when": {
        "chain_west": "true"
      }
    }
  ]
}